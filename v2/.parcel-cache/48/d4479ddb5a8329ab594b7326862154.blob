    }
    ul.book__place-options li:hover {
	    background: #DADEE7;
    }
    .show
    { 
       display: block; 
    }
    .validationFailed{
	    border: red 1px solid;
	    border-radius: 8px;
    }
    .validationPartedPassed{
	    border: magenta 1px solid;
	    border-radius: 8px;
    }
    .customPlace-hidden{
	    display: none;
	    animation: ease hidden 0.2s;
    }
    .customPlace-visible{
	    display: block;
	    animation: ease visible 0.2s;
    }
    @keyframes visible {
	    from { opacity:0;height: 0px; }
	    to { opacity:100;height: 60px; }
    }
    @keyframes hidden {
	    from { opacity:100;height: 60px; }
	    to { opacity:0;height: 0px; }
    }
    .book__field-cost-option {
	    color: red;
    }
