    .book__btn {
	      color: #fff;
	      background: #000000;
	      padding: 6px 12px;
	      margin: 12px 0;
	      text-align: center;
	      display: block;
	      width: 100%;
	      height: 60px;
	      border: none;
	      border-radius: 8px;
	      transition-delay: 0.05s;
	      transition-duration: 0.25s;
      transition-property: all;
      }
    .book__btn:hover {
	    background: #F7B903;
	    box-shadow: 16px 26px 25px -25px #F7B903;
	    
	    cursor: pointer;
    }
    .book__btn_send {
	    color: #fff;
	    background: #000000;
	    padding: 6px 12px;
	    margin: 12px 0;
	    text-align: center;
	    display: block;
	    width: 100%;
	    height: 60px;
	    border: none;
	    border-radius: 8px;
	    transition-delay: 0.05s;
	    transition-duration: 0.25s;
    transition-property: all;
    }
    .book__radio {
	    font-size: 13px;
    }
    .book__checkup {
	    font-size: 14px;
	    color: #959ba4;
	    margin: 32px 0;
    }
    .book__place-options {
	    display: none;
    list-style-type: none;
    margin: -8px 0 0 0;
    padding: 8px 0 0 0;
    background: #ffffff;
    border: 1px solid #000000;
    border-top: none;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 8px;
    transition: all 0.2s;
    box-shadow: 16px 26px 25px -25px #abb6c5;
