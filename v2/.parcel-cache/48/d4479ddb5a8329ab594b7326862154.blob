      }
    .dp-year {
	display: block;
	padding: 8px 40px;
	width: 100%;
      }
    .dp-edge-day {
	color: #AAA;
      }
    .dp-day:hover,
      .dp-month:hover,
      .dp-year:hover,
      .dp-current:focus,
      .dp-current,
      .dp-day:focus,
      .dp-month:focus,
      .dp-year:focus {
	outline: none;
	background: #ffd452;
	color: white;
      }
    .dp-selected:hover,
      .dp-selected:focus,
      .dp-selected {
	background: #F7B903;
	color: #FFF;
      }
    .dp-day-disabled {
	background: transparent;
	text-decoration: line-through;
    
	color:#969699;
      }
    .dp-day-disabled:focus,
      .dp-day-disabled:hover {
	background: #DDD;
      }
    .dp-focuser {
	position: absolute;
	z-index: 0;
	top: 50%;
	left: 50%;
      }
