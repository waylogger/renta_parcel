    .book__checkbox {
	    position: absolute;
	    z-index: -1;
	    opacity: 0;
	    cursor: pointer;
      }
    .book__checkbox+label {
	    display: inline-flex;
	    align-items: center;
	    user-select: none;
	    cursor: pointer;
	    margin: 6px 0;
      }
    .book__checkbox+label::before {
	    content: '';
	    display: inline-block;
	    width: 24px;
	    height: 24px;
	    flex-shrink: 0;
	    flex-grow: 0;
	    border: 1px solid #DADEE7;
	    border-radius: 4px;
	    margin-right: 12px;
	    background-repeat: no-repeat;
	    background-position: center center;
	    background-size: 50% 50%;
	    cursor: pointer;
      }
    .book__checkbox:checked+label::before {
	    border-color: #F9B702 ;
	    background-color: #F9B702 ;
	    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 8 8'%3e%3cpath fill='%23fff' d='M6.564.75l-3.59 3.612-1.538-1.55L0 4.26 2.974 7.25 8 2.193z'/%3e%3c/svg%3e");
      }
    .book__checkbox:not(:disabled):not(:checked)+label:hover::before {
	    background-color: #fff2d0;
	    border-color: #ffe7ab;
      }
    .book__checkbox:not(:disabled):active+label::before {
	    background-color: #fff2d0 ;
	    border-color: #ffe7ab ;
      }
    .book__checkbox:focus:not(:checked)+label::before {
	    border-color: #ffe7ab;
      }
    .book__checkbox:disabled+label::before {
	    background-color: #fff2d0;
      }
    #lawAgreement {
	  margin-top: 16px;
